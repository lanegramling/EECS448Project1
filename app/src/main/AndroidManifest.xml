<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wubbalubbadubdub.eecs448project1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Wubba Lubba Dub Dub's Event Scheduler"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectUserActivity" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".AddEventActivity" android:screenOrientation="landscape" />
        <activity android:name=".ListActivity" android:screenOrientation="landscape" />
        <activity android:name=".ViewActivity" android:screenOrientation="landscape"></activity>
    </application>

</manifest>